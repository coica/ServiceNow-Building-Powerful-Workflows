<?xml version="1.0" encoding="UTF-8"?><record_update table="sys_script_fix">
    <sys_script_fix action="INSERT_OR_UPDATE">
        <active>true</active>
        <before>false</before>
        <description/>
        <flush_cache>false</flush_cache>
        <name>SetRoomRefToBlankWhenIsToDelete</name>
        <record_for_rollback>false</record_for_rollback>
        <run_once>true</run_once>
        <script><![CDATA[	var gr = new GlideRecord('x_hotel_check_in');
	gr.query();   //get all check_in
	while(gr.next()){ 
		var referencedRoom = gr.room.getRefRecord();
		gs.info("ACO - STRYxxxxx - SetRoomRefToBlankWhenIsToDelete : " + referencedRoom.status);
		if (referencedRoom.status == "todelete"
		   ){
			
			gs.info("ACO - STRYxxxxx - SetRoomRefToBlankWhenIsToDelete set to blanks: " + 
				referencedRoom.status);
			    gr.room = "";
				gr.update();
			
		}
		
	}]]></script>
